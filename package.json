{
  "name": "n8n-nodes-bimx-ifc-qto",
  "version": "1.3.02",
  "description": "Custom BIMX IFC Quantity Takeoff nodes for n8n",
  "license": "MIT",
  "author": "BIM X Consulting",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && node ./scripts/copy-wasm.cjs",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*",
    "nodes/**/*",
    "credentials/**/*",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "n8n-community-node-package",
    "bimx",
    "ifc",
    "qto",
    "revit",
    "bim"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "exceljs": "^4.4.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "web-ifc": "^0.0.47",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^18.17.0"
  },
  "n8n": {
    "nodes": [
      {
        "type": "node",
        "name": "dist/nodes/BimxIfcSpaceQto.node.js",
        "className": "BimxIfcSpaceQto"
      },
      {
        "type": "node",
        "name": "dist/nodes/IfcParameterExplorer.node.js",
        "className": "IfcParameterExplorer"
      },
      {
        "type": "node",
        "name": "dist/nodes/BimxIfcAttributeExport.node.js",
        "className": "BimxIfcAttributeExport"
      },
      {
        "type": "node",
        "name": "dist/nodes/BimxTableFilter.node.js",
        "className": "BimxTableFilter"
      },
      {
        "type": "node",
        "name": "dist/nodes/BimxWatch.node.js",
        "className": "BimxWatch"
      },
      {
        "type": "node",
        "name": "dist/nodes/BimxRuleValidator.node.js",
        "className": "BimxRuleValidator"
      },
      {
        "type": "node",
        "name": "dist/nodes/BimxBcsvBuilder.node.js",
        "className": "BimxBcsvBuilder"
      }
    ],
    "credentials": []
  }
}
